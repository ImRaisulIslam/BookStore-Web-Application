@model  IEnumerable<BookModel>

@{
    ViewData["Title"] = "All Books";
}





<div class="container">
    <h2>All Book List</h2>
    <div class="row">


        @foreach (var book in  Model)
        {
            <div class="col-md-4">
                <partial name="_bookthumbnail" model="book"/>
            </div>
        }



    </div>
</div>
